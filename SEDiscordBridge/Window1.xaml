<Window x:Class="SEDiscordBridge.Window1"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SEDiscordBridge"
        mc:Ignorable="d"
        Title="Window1" Height="477" Width="489">
    <Grid>
        <DataGrid HorizontalAlignment="Left" Height="401" Margin="10,10,0,0" VerticalAlignment="Top" Width="461" x:Name="DataGrid" AutoGenerateColumns="False">
            <DataGrid.Columns>
                <DataGridTextColumn Binding="{Binding Path=Value}" 
                                Foreground="Black" Width="450" Header="Message" IsReadOnly="false" />
            </DataGrid.Columns>
        </DataGrid>
        <Button Content="Remove" HorizontalAlignment="Left" Margin="396,416,0,0" VerticalAlignment="Top" Width="75" Click="Button_Click_1"/>
        <Button Content="Add" HorizontalAlignment="Left" Margin="316,416,0,0" VerticalAlignment="Top" Width="75" Click="Button_Click"/>

    </Grid>
</Window>
